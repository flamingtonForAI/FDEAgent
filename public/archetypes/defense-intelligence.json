        "type": "optimization",
        "description": {
          "en": "AI-optimized logistics routing",
          "cn": "AI优化后勤路线"
        },
        "enabledActions": [
          "AI Optimize Route"
        ],
        "modelConfig": {
          "modelType": "route-optimizer",
