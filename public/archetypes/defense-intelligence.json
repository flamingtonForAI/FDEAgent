          "en": "End-to-end target identification to engagement workflow",
          "cn": "目标识别到交战的端到端流程"
        },
        "trigger": {
          "type": "event",
          "config": {
            "eventType": "target_detected"
          }
        },
        "steps": [
          {
            "id": "detect",
            "name": "Detect Target",
            "description": {
              "en": "Sensor detection",
              "cn": "传感器检测"
            },
            "type": "action",
            "nextSteps": [
              "classify"
            ]
          },
          {
            "id": "classify",
            "name": "Classify Target",
            "description": {
              "en": "AI classification",
              "cn": "AI分类"
            },
            "type": "action",
            "nextSteps": [
              "assess"
            ]
          },
          {
            "id": "assess",
            "name": "Threat Assessment",
            "description": {
              "en": "Assess threat level",
              "cn": "威胁评估"
            },
            "type": "action",
            "nextSteps": [
              "decide"
            ]
          },
          {
            "id": "decide",
            "name": "Decision",
            "description": {
              "en": "Commander decision",
              "cn": "指挥官决策"
            },
            "type": "action"
          }
        ],
        "entryStep": "detect",
        "roles": [
          "Sensor Operator",
          "Intelligence Analyst",
          "Commander"
        ],
        "sla": {
          "maxDuration": "30m",
          "escalationPath": [
            "Watch Officer",
            "Commander"
          ]
        }
      }
    ],
    "rules": [
      {
        "id": "hostile-escalation",
        "name": "Hostile Target Escalation",
        "description": {
          "en": "Escalate high-threat hostile targets",
          "cn": "高威胁敌方目标升级"
        },
        "type": "trigger",
        "appliesTo": [
          "target"
        ],
        "expression": "classification === \"Hostile\" && threatLevel === \"High\""
      }
    ],
    "aiCapabilities": [
      {
        "id": "sensor-fusion",
        "name": "Multi-Sensor Target Fusion",
